<h1>Listing oauth_clients</h1>

<table>
  <tr>
    <th><%= t('activeldap.attributes.oauth_client.displayName') %></th>
    <th><%= t('activeldap.attributes.oauth_client.puavoOAuthClientId') %></th>
  </tr>

<% @oauth_clients.each do |oauth_client| %>
  <tr>
    <td><%= link_to oauth_client.displayName, oauth_client %></td>
    <td><%=h oauth_client.puavoOAuthClientId %></td>
    <td><%=h oauth_client.puavoOAuthAccess %></td>
    <td><%= link_to 'Show', oauth_client %></td>
    <td><%= link_to 'Edit', edit_oauth_client_path(oauth_client) %></td>
    <td><%= link_to t('link.destroy'), oauth_client, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New oauth_client', new_oauth_client_path %>
