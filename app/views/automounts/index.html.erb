<h1>Listing automounts</h1>

<table>
  <tr>
    <th>Cn</th>
  </tr>

<% @automounts.each do |automount| %>
  <tr>
    <td><%=h automount.cn %></td>
    <td><%= link_to 'Show', automount %></td>
    <td><%= link_to 'Edit', edit_automount_path(automount) %></td>
    <td><%= link_to 'Destroy', automount, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New automount', new_automount_path %>
