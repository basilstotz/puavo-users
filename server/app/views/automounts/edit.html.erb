<h1>Editing automount</h1>

<% form_for([@server, @automount]) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :cn %><br />
    <%= f.text_field :cn %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', [@server, @automount] %> |
<%= link_to 'Back', server_automounts_path(@server) %>
